{
    "name": "file-from-template",
    "displayName": "File from Template",
    "description": "Create new file from templates",
    "version": "0.1.0",
    "author": "Bruno Paz",
    "publisher": "brpaz",
    "homepage": "https://github.com/brpaz/vscode-file-from-template",
    "repository": {
        "type": "git",
        "url": "https://github.com/brpaz/vscode-file-from-template.git"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.3.0"  
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.newFileFromTemplate"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.newFileFromTemplate",
            "title": "New File from template",
            "category": "Files"
        }],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.newFileFromTemplate"
                }
            ]
        }
    },
    
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}